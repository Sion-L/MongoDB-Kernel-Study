{
    "root": ".",
    "links": {
        "sidebar": {
            "github": "https://github.com/pengzhenyi2015/MongoDB-Kernel-Study"
        }
    },
    "plugins": [
        "page-treeview",
        "hide-element",
        "back-to-top-button",
        "chapter-fold",
        "code",
        "splitter",
        "-lunr", 
        "-search", 
        "search-pro",
        "pageview-count",
        "tbfed-pagefooter",
        "popup"
